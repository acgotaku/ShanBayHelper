{
   "background": {
      "scripts": ["js/jquery-2.1.1.min.js","background.js"],
      "persistent": false
   },
   "content_scripts": [ {
      "js": [ "js/jquery-2.1.1.min.js","js/howler.min.js","js/shanbay.js","js/readability.js"],
      "css": [ "css/shanbay.css"],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "default_locale": "en",
   "description": "__MSG_description__",
   "icons": {
      "32": "images/logo32.png",
      "16": "images/logo16.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
   },
   "browser_action": {
      "default_icon": "images/logo32.png",
      "default_popup": "popup.html",
      "default_title": "__MSG_appName__"
   },
   "web_accessible_resources":[
      "css/shanbay.css"  
   ],
   "manifest_version": 2,
   "permissions": [ "cookies", "contextMenus", "activeTab", "tabs", "unlimitedStorage","notifications", "*://*/*"],
   "name": "__MSG_appName__",
   "version": "0.0.1"
}
